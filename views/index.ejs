<html>
    <%- include('./parts/head.ejs') %> 
    <body>
        
        <form action="/" method="POST">
            <input type="text" name="name" placeholder="name">
            <input type="number" name="age" placeholder="age">
            <input type="submit" value="submit">
        </form> 

        <ul>
            <% for (let user of users) { %>
            <li>
                <%= user.name %> : <%= user.age %>
            </li>
            <% } %>
        </ul>
    </body> 
</html>

