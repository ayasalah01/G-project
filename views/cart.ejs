	<body className='snippet-body'>
                                <div class="container mt-5 mb-5">
            <div class="d-flex justify-content-center row">
                <div class="col-md-9">
                    <div class="p-2">
                        <h4>Shopping cart</h4>
                    </div>
                    <% for (let i = 0; i < data.length; i++ ) { %>
                    <div class="d-flex flex-row justify-content-between align-items-center p-2 bg-white mt-4 px-3 rounded">
                        <div class="mr-1"><img class="rounded" src="<%=data[i].image%> " width="80" height="50"></div>
                        <div class="d-flex flex-column align-items-center product-details">
                            <span class="font-weight-bold" >
                                <h4 style="color: #023859;">
                                    <%=data[i].service%> 
                                </h4>
                            </span>
                            <div class="d-flex flex-row product-desc">
                                <div class="size mr-1">
                                    <span class="font-weight-bold">Price:</span>
                                    <span class="font-weight-bold">&nbsp;<%=data[i].price%></span>
                                </div>
                            </div>
                        </div>
                        <div>
                            <form  method="POST">
                                <span class="font-weight-bold">
                                    <h5 style="color: #023859;">
                                        Amount :
                                    </h5>
                                </span>
                                <input type="hidden" name="cartId" value="<%= data[i]._id%>">
                                <input type="number" name="amount" value="<%= data[i].amount%>">
                                <input type="submit" class="pay-button" style="border-radius: 10px;" value="save" formaction="/cart/update" >
                            
                        </div>
                        <div class="size mr-1">
                            <span class="font-weight-bold">
                                <h5 style="color: #023859;">
                                    Total Price :
                                </h5>
                            </span>
                            <span class="font-weight-bold">&nbsp;<%= data[i].amount * data[i].price%></span>
                        </div>
                        <div class="d-flex align-items-center">
                            <i class="fa fa-trash mb-1 text-danger">
                                <input type="submit"  class="pay-button" value="d" formaction="/cart/delete" >
                            </i>
                        </div>
                    </div>
                    <% } %>
                    <div class="d-flex flex-row align-items-center mt-3 p-2 bg-white rounded">
                        <a class="btn btn-warning btn-block btn-lg ml-2 pay-button" type="button" href="/pay" >Proceed to Pay</a>
                        <!-- <button class="btn btn-warning btn-block btn-lg ml-2 pay-button" type="button">Proceed to Pay</button></div> -->
                </div>
            </form>
                
            </div>
        </div>
	
