<!doctype html>
<html class="no-js" lang="zxx">
<%- include('./parts/homehead.ejs')%>
<body>
    <!-- header-start -->
        <header>
            <div class="header-area ">
                <div id="sticky-header" class="main-header-area">
                    <div class="container-fluid p-0">
                        <div class="row align-items-center no-gutters">
                            <div class="col-xl-2 col-lg-2">
                                <div class="logo-img">
                                    <a href="">
                                        <img src="img/logo (2).png" alt="">
                                    </a>
                                </div>
                            </div>
                            <div class="col-xl-5 col-lg-6">
                                <div class="main-menu  d-none d-lg-block">
                                    <nav>
                                        <ul id="navigation">
                                            <li><a href="/HomeSPAfterlogin">Home</a></li>
                                            <li><a href="Inbox.html">Inbox</a></li>
                                            <li><a class="active" href="/spProfile">Your Profile</a></li>
                                            <li><a href="/spSetting">Settings</a></li>
                                        </ul>
                                    </nav>
                                </div>
                            </div>
                            
                            <div class="col-12">
                                <div class="mobile_menu d-block d-lg-none"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
<!-- header-end -->

        <div class="row">
                    <div class="col-xl-12">
                        <div class="section_title text-center mb-100">
                        </div>
                    </div>
                </div>

<div class="container">
<div class="main-body">
<div class="row gutters-sm">
<div class="col-md-4 mb-3">
<div class="card">
<div class="card-body">
<div class="d-flex flex-column align-items-center text-center">
<img src="img/comment/comment_2.png" alt="Name" class="rounded-circle" width="100">
<div class="mt-3">
<h4 style="color:#023859"><%= user.username %></h4>
<p class="text-secondary mb-1">About</p>
</div>
</div>
</div>
</div>
</div>
<div class="col-md-8">
<div class="card mb-3">
<div class="card-body">
<div class="row">
<div class="col-sm-3">
<h6 class="mb-0">userName</h6>
</div>
<div class="col-sm-9 text-secondary">
    <%= user.username %>
</div>
</div>
<hr>
<div class="row">
<div class="col-sm-3">
<h6 class="mb-0">Email</h6>
</div>
<div class="col-sm-9 text-secondary">
    <%= user.email %>
</div>
</div>
<hr>
<div class="row">
<div class="col-sm-3">
<h6 class="mb-0">Address</h6>
</div>
<div class="col-sm-9 text-secondary">
    <%= user.Address %>
</div>
</div>
<hr>
<div class="row">
<div class="col-sm-3">
<h6 class="mb-0">Category</h6>
</div>
<div class="col-sm-9 text-secondary">
    <%= user.category %>
    </div>
</div>
<hr>
<div class="row">
<div class="col-sm-12">
<a class="btn btn-info " target="__blank" href="/spSetting">Edit</a>
</div>
</div>
</div>
</div>
</div>
</div>
</div>
</div>
<!-- form itself end-->
<form action="/logout" id="log-out-form" class="white-popup-block mfp-hide" method="post">
    <div class="popup_box ">
            <div class="popup_inner">
                <h3>Are You Sure You Want To Sign Out?</h3>
                    <a href="/logout" class="boxed-btn3">Yes</a>
                        </div>
                    </div>
                </form>
            </div>
        </div>
</form>
<style type="text/css">
        body{
       margin-top:20px;
       color: #023859;
       text-align: left;
       background-color: #f1f1e6;    
   }
   .main-body {
       padding: 15px;
   }
   .card {
       box-shadow: 0 1px 3px 0 #023859, 0 1px 2px 0 #023859;
   }
   
   .card {
       position: relative;
       display: flex;
       flex-direction: column;
       min-width: 0;
       word-wrap: break-word;
       background-color: #fff;
       background-clip: border-box;
       border: 0 solid #023859;
       border-radius: .25rem;
   }
   
   .card-body {
       flex: 1 1 auto;
       min-height: 1px;
       padding: 1rem;
   }
   
   .gutters-sm {
       margin-right: -8px;
       margin-left: -8px;
   }
   
   .gutters-sm>.col, .gutters-sm>[class*=col-] {
       padding-right: 8px;
       padding-left: 8px;
   }
   .mb-3, .my-3 {
       margin-bottom: 1rem!important;
   }
   
   .bg-gray-300 {
       background-color: #daa520;
   }
   .h-100 {
       height: 100%!important;
   }
   .shadow-none {
       box-shadow: none!important;
   }
   .mt-3 a{
       color:#daa520;
       background-color:#023859;
       border:none;
   }
   .mt-3 a:hover{
       color:#fff;
       background-color:#daa520;
       border:none;
   }
   .col-sm-12 a{
       color:#daa520;
       background-color:#023859;
       border:none;
   }
   .col-sm-12 a:hover{
       color:#fff;
       background-color:#daa520;
       border:none;
   }
   .mb-0{
       color:#023859;
   }
        </style>
